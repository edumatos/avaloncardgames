{
"manifest_version": 2,
  "name": "Avalon Spider Solitaire",
  "description": "Avalon Spider Solitaire",
  "version": "2",
  "minimum_chrome_version": "23.0.1261",
  "offline_enabled": true,
  "app": {


    "launch": {
      "local_path": "index.htm",
      "container": "panel"
    }
  },
  "icons": {
    "16": "ChromePreview16.png",
    "128": "ChromePreview128.png"
  },
  
  
  "content_security_policy": "script-src 'unsafe-eval' 'self' ; object-src 'self'  'unsafe-eval'" ,
  
  
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "background"
  ]
  

}